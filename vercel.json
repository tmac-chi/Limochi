{
  "functions": {
    "server/index.ts": {
      "runtime": "@vercel/node@3.0.18"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "builds": [
    {
      "src": "client/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist",
        "buildCommand": "npm run build --workspace client"
      }
    }
  ],
  "installCommand": "npm install"
}
